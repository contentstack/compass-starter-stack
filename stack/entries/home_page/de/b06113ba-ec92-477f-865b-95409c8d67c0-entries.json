lt.","image":{"uid":"blt0e70ce2db762e69a","created_at":"2025-04-24T11:47:38.680Z","updated_at":"2025-04-24T11:47:38.680Z","content_type":"image/webp","file_size":"524200","tags":[],"filename":"card-treestairs.webp","url":"https://images.contentstack.io/v3/assets/<api_key>/<asset_uid>/<entity_uid>/<asset_name>","ACL":[],"is_dir":false,"parent_uid":"blt351a872177d863dc","_version":1,"title":"card-treestairs.webp","publish_details":[{"environment":"bltb377ab306d29e646","locale":"es","time":"2025-04-24T11:48:56.518Z","user":"userUID","version":1},{"environment":"bltb73a6fe374802283","locale":"es","time":"2025-04-24T11:48:56.544Z","user":"userUID","version":1},{"environment":"bltb377ab306d29e646","locale":"fr","time":"2025-04-24T11:48:56.564Z","user":"userUID","version":1},{"environment":"bltb73a6fe374802283","locale":"fr","time":"2025-04-24T11:48:56.583Z","user":"userUID","version":1},{"environment":"bltb377ab306d29e646","locale":"de","time":"2025-04-24T11:48:56.599Z","user":"userUID","version":1},{"environment":"bltb73a6fe374802283","locale":"de","time":"2025-04-24T11:48:56.635Z","user":"userUID","version":1},{"environment":"bltb377ab306d29e646","locale":"en","time":"2025-04-24T11:48:56.665Z","user":"userUID","version":1},{"environment":"bltb73a6fe374802283","locale":"en","time":"2025-04-24T11:48:56.677Z","user":"userUID","version":1}]},"image_alt_text":"Alt-Text für Bild im Abschnitt","is_thumbnail":false,"cta":[{"text":"Mehr sehen","_metadata":{"uid":"cs235fee6fb15cc20a"},"link":[{"uid":"bltabd593c726fec533","_content_type_uid":"article_listing_page"}],"external_url":""}],"styles":{"theme":"light"}}],"_metadata":{"uid":"cs55bbefae1665aeec"},"styles":{"image_position":"left"}}}],"featured_articles":{"heading":"Ausgewählte Artikel","sub_heading":"","articles":[{"uid":"bltfc76c416c6bb73ba","_content_type_uid":"article"},{"uid":"blt8fee2d1390245699","_content_type_uid":"article"},{"uid":"blt7b1799b645438265","_content_type_uid":"article"},{"uid":"blta038471456aef7a5","_c